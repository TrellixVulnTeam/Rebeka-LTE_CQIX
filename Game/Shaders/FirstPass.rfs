#version 400
in vec3 position;
in vec3 normal;
in vec2 texcoord;
uniform sampler2D difftext;


layout(location = 0) out vec3 colmap;
layout(location = 1) out vec3 posmap;
layout(location = 2) out vec3 normap;




void main()
{
colmap = texture2D(difftext, texcoord).xyz;
posmap = position;
normap = normal;
}
